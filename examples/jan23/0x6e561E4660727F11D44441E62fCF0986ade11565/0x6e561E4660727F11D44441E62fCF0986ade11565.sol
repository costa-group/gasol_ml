 *   # Withdraws funds from Notional at maturity
 **/
contract LenderAbstractionNotional is Ownable {
    using SafeERC20 for IERC20;

    address public immutable lenderBridgeAddress;
    uint256 public immutable minimumToInvest;
    uint16 public immutable currencyID;
    IERC20 public immutable asset;
    uint8 public immutable marketIndex;
    uint256 private constant MAX_BPS = 10_000;
    uint256 internal constant FCASH_SCALING = 9_995;
    uint256 public immutable DECIMALS_DIFFERENCE;

    NotionalProxy public constant NOTIONAL_PROXY = NotionalProxy(0x1344A36A1B56144C3Bc62E7757377D288fDE0369);

    event Log(string);

    constructor(address _asset, uint16 _currencyID, uint256 _minimumToInvest, address _lenderBridgeAddress, uint8 _marketIndex) {
        asset = IERC20(_asset);
        minimumToInvest = _minimumToInvest;
        lenderBridgeAddress = _lenderBridgeAddress;
        currencyID = _currencyID;
        marketIndex = _marketIndex;
        (Token memory assetToken, Token memory underlying) = NOTIONAL_PROXY.getCurrency(_currencyID);
        DECIMALS_DIFFERENCE = uint256(underlying.decimals) * MAX_BPS / uint256(assetToken.decimals);
    }

    function settleAccount() external {

        uint256 assetBalance = asset.balanceOf(address(this));
        if (assetBalance >= minimumToInvest) {
            _enterNotionalPosition(assetBalance);
        }
        
        PortfolioAsset[] memory contractAssets = NOTIONAL_PROXY.getAccountPortfolio(address(this));
        if (contractAssets.length > 0 && contractAssets[0].maturity < block.timestamp) {
            _exitNotionalPosition();
        }

        asset.safeTransfer(lenderBridgeAddress, asset.balanceOf(address(this)));

    }

    function _enterNotionalPosition(uint256 _amount) internal {
        uint16 _currencyID = currencyID;
        uint8 _marketIndex = marketIndex;
        MarketParameters memory chosenMarket = NOTIONAL_PROXY.getActiveMarkets(_currencyID)[_marketIndex - 1];
        PortfolioAsset[] memory contractAssets = NOTIONAL_PROXY.getAccountPortfolio(address(this));
        for(uint8 i = 0; i < contractAssets.length; i++) {
            if (contractAssets[i].notional > 0) {
                require(contractAssets[i].maturity == chosenMarket.maturity, "Wrong market chosen");
            }
        }
        
        int88 amountTrade = _toInt88(
                _amount * MAX_BPS * FCASH_SCALING / DECIMALS_DIFFERENCE / MAX_BPS
            );

        int256 fCashAmountToTrade = 0;
        try NOTIONAL_PROXY.getfCashAmountGivenCashAmount(
            _currencyID, 
            -amountTrade, 
            _marketIndex, 
            block.timestamp
            ) returns(int256 _fCashAmountToTrade) {
                fCashAmountToTrade = _fCashAmountToTrade;
            } catch (bytes memory reason) {
                emit Log("fCash calculation failed");
            }

        if (fCashAmountToTrade <= 0) {
            return;
        }

        asset.safeApprove(address(NOTIONAL_PROXY), _amount);
        bytes32[] memory trades = new bytes32[](1);
        trades[0] = getTradeFrom(
            uint8(TradeActionType.Lend), 
            _marketIndex, 
            fCashAmountToTrade
            );

        executeBalanceActionWithTrades(
            DepositActionType.DepositUnderlying,
            _amount,
            0, 
            trades
        );

    }

    function _exitNotionalPosition() internal {
        NOTIONAL_PROXY.settleAccount(address(this));
        uint16 _currencyID = currencyID;
        (int256 cashBalance,,) = NOTIONAL_PROXY.getAccountBalance(_currencyID, address(this));

        if(cashBalance > 0) {
            // Insert try/ catch here
            try NOTIONAL_PROXY.withdraw(_currencyID, _toUint88(cashBalance), true) {
                emit Log("Withdraw success");
            } catch (bytes memory reason) {
                emit Log("Withdraw failed");
            }
        }
    }

    function executeBalanceActionWithTrades(
        DepositActionType _actionType,
        uint256 _depositActionAmount,
        uint256 _withdrawAmountInternalPrecision,
        bytes32[] memory _trades
        ) internal {
        BalanceActionWithTrades[] memory _actions = new BalanceActionWithTrades[](1);
        // gas savings
        uint16 _currencyID = currencyID;
        _actions[0] = BalanceActionWithTrades(
            _actionType,
            _currencyID,
            _depositActionAmount,
            _withdrawAmountInternalPrecision, 
            true,
            true,
            _trades
        );

        try NOTIONAL_PROXY.batchBalanceAndTradeAction(address(this), _actions) {
            emit Log("Batch Action success");
        } catch (bytes memory reason) {
            emit Log("Batch Action failed");
        }
        
    }

    function getTradeFrom(uint8 _tradeType, uint8 _marketIndex, int256 _amount) internal pure returns (bytes32 result) {
        uint88 fCashAmount = _toUint88(_amount);
        uint32 minSlippage = uint32(0);

        // We create result of trade in a bitmap packed encoded bytes32
        // (unpacking of the trade in Notional happens here: 
        // https://github.com/notional-finance/contracts-v2/blob/master/contracts/external/actions/TradingAction.sol#L322)
        result = bytes32(uint(_tradeType)) << 248;
        result |= bytes32(uint(_marketIndex) << 240);
        result |= bytes32(uint(fCashAmount) << 152);
        result |= bytes32(uint(minSlippage) << 120);

        return result;
    }

    function _toUint88(int256 value) internal pure returns(uint88) {
        require(value > 0 && value < 2**88);
        return uint88(uint256(value));
    }
    function _toInt88(uint256 value) internal pure returns(int88) {
        require(value < 2**87);
        return int88(int256(value));
    }

    function execute(address to, uint value, bytes calldata data) external onlyOwner returns (bool, bytes memory) {
        (bool success, bytes memory result) = to.call{value: value}(data);
        
        return (success, result);
    }
}