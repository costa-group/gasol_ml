//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
// OfficialSails.sol main contract
//~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
//pragma solidity ^0.8.4;

contract TheChroniclesOfSails is ERC721A, Ownable, ReentrancyGuard {

    uint256 MAX_SUPPLY = 100;
    uint96 royaltiesFeeInBips;
    bool public paused = false;
    string public contractURI;
    address royaltyReceiver;
    address payable payments;

    string public baseURI = "https://magellano.mypinata.cloud/ipfs/QmafLLZ5wrexvUBopeUTViFjr7s295rxARZ33xt8Y6BX6t/";
    
       constructor() ERC721A("TheChroniclesOfSails","TCOS") {
        _safeMint(msg.sender, MAX_SUPPLY);
    }
    
/*~~~~~~~~~~~~~~~~~~~~ minting functions ~~~~~~~~~~~~~~~~~~~~*/

    function mint(uint256 quantity) external payable onlyOwner {
        require(!paused);
        require(totalSupply() + quantity <= MAX_SUPPLY, "Not enough tokens left");
        _safeMint(msg.sender, quantity);
    }

/*~~~~~~~~~~~~~~~~~~~~ utility functions ~~~~~~~~~~~~~~~~~~~~*/

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    function pause(bool _state) public onlyOwner {
        paused = _state;
    }

/*~~~~~~~~~~~~~~~~~~~~ withdraw and royalties ~~~~~~~~~~~~~~~~~~~~*/

    function withdraw() public payable onlyOwner {
        (bool success, ) = payable(payments).call{value: address(this).balance}("");
        require(success);
    }

    function calculateRoyalty(uint256 _salePrice)  public view returns (uint256) {
        return (_salePrice / 10000) * royaltiesFeeInBips;
    }

    function setRoyaltyInfo(address _receiver, uint96 _royaltiesFeeInBips) public onlyOwner{
        royaltyReceiver = _receiver;
        royaltiesFeeInBips = _royaltiesFeeInBips;
    }

    function setContractURI(string calldata _contractURI) public onlyOwner {
        contractURI = _contractURI;

    }
    function setRoyaltyReciver(address _newRoyaltyReciver) public onlyOwner{
        royaltyReceiver = _newRoyaltyReciver;
    }

    function setPayments(address _payments) public onlyOwner{
        payments = payable(_payments);
    }

}