// File: contracts/art/SecondLiveArtMinter.sol

//pragma solidity ^ 0.8.0;








contract SecondLiveArtMinter is IAvatarMinter, Ownable, ReentrancyGuard {
    
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    bool private initialized;
    EnumerableSet.UintSet private tidSet;
    
    uint private seed;

    ISecondLive.Attribute public attribute;
    ClaimData public claimData;
    uint256 public soldAmount;
    mapping(address => uint256) public userBoughtAmount;
    
    ISecondLive public avatar;

    event AddTokenIds(uint fromId, uint toId);
    event Claim(address sender, uint256 amount);
    event UpdateClaimData(ClaimData _claimData);
    event UpdateAttribute(ISecondLive.Attribute _attribute);
    event Withdraw(address withdrawAddr, uint256 amount);

    function initialize(
        address _owner,
        ISecondLive _avatar,
        ClaimData calldata _claimData,
        ISecondLive.Attribute calldata _attribute) external {
        require(!initialized, "initialize: Already initialized!");
        
        _transferOwnership(_owner);
        
        avatar = _avatar;
        claimData = _claimData;
        attribute = _attribute;

        initialized = true;
    }

    function claim(uint256 amount, uint256 addressId, bytes32[] memory merkleProof) external override payable nonReentrant {
        require(block.timestamp >= claimData.startTime, "not start!");
        require(block.timestamp < claimData.endTime, "end!");
       
        bytes32 merkleRoot = claimData.whitelistRoot;
        if (merkleRoot != bytes32(0)) {
            bytes32 node = keccak256(abi.encodePacked(addressId, msg.sender));
            require(MerkleProof.verify(merkleProof, merkleRoot, node), "SecondLive: sender can not claim");
        }

        uint256 boughtAmount = userBoughtAmount[msg.sender];
        require(claimData.maxAmount.sub(boughtAmount) >= amount, "excessive amount");
        require(claimData.totalAmount.sub(soldAmount) >= amount, "sold out");
        
        if (claimData.unitPrice > 0) {
            uint256 total = amount.mul(claimData.unitPrice);
            require(msg.value >= total,"value error");
            uint256 returnBack = msg.value.sub(total);
            if(returnBack > 0) {
                payable(msg.sender).transfer(returnBack);
            }
            payable(0x37593E4c28Ef8a7ab9F5A7be1bF7f89347172dC6).transfer(total);
        }else{
            require(msg.value == 0,"value error");
        }

        for (uint256 i = 0; i < amount; i++) {
            
            seed = uint(keccak256(
                abi.encodePacked(seed, _msgSender(), i)));
            uint seedBase = tidSet.length();
            uint index = seed%seedBase;
            uint tokenId = tidSet.at(index);
            tidSet.remove(tokenId);
            attribute.quality = tokenId.sub(1).div(10).add(1);
            attribute.extra = tokenId;
            avatar.mint(msg.sender, tokenId, attribute);
        }
        
        userBoughtAmount[msg.sender] = userBoughtAmount[msg.sender].add(amount);
        soldAmount = soldAmount.add(amount);

        emit Claim(msg.sender, amount);
    }

    function addTid(uint fid, uint tid) external onlyOwner {
        for(uint i = fid; i < tid; i++) {
            tidSet.add(i);
        }
        emit AddTokenIds(fid, tid);
    }

    function noTakenAwayTids() external view returns (uint256[] memory) {
        return tidSet.values();
    }

    function noTakenAwayNum() external view returns (uint256) {
        return tidSet.length();
    }

    function updateClaimData(ClaimData calldata _claimData) external onlyOwner {
        claimData = _claimData;
        emit UpdateClaimData(claimData);
    }

    function updateAttribute(ISecondLive.Attribute calldata _attribute) external onlyOwner {
        attribute = _attribute;
        emit UpdateAttribute(attribute);
    }

    receive() payable external {}
    
}