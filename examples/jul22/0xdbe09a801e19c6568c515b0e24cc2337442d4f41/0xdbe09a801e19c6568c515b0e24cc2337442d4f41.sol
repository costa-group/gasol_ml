// File: contracts/WireNodes.sol



//pragma solidity ^0.8.0;





// //import "hardhat/console.sol";




contract WireNodes is ERC1155 {

    string public symbol = "WireNode";

    address public contractOwner;

    uint256 public decimals = 18;

    struct NodeInfo {

        uint256 id; // NFT id

        uint256 totalSupply; // how many minted

        uint256 maxSupply; 

        uint256 price; // cost required to claim NFT

        bool exists;

    }



    mapping(uint256 => NodeInfo) public nodeInfo; // retains each node info

    mapping(uint256 => string) internal uris;

    address public dataFeedAddress = 0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419;

    /**

     * 100500000

     * 1.005 %

     */

    uint32 public slippage = 500000;



    constructor(address _contractOwner) ERC1155("") {

        // sets owner

        contractOwner = _contractOwner;

        // make the nodes

        nodeInfo[0] = NodeInfo({

            id: 1,

            totalSupply: 0,

            maxSupply: 21,

            price: 750000 * (10**decimals),

            exists: true

        });

        nodeInfo[1] = NodeInfo({

            id: 2,

            totalSupply: 0,

            maxSupply: 84,

            price: 100000 * (10**decimals),

            exists: true

        });

        nodeInfo[2] = NodeInfo({

            id: 3,

            totalSupply: 0,

            maxSupply: 1000,

            price: 10000 * (10**decimals),

            exists: true

        });

    }



    modifier onlyOwner() {

        require(msg.sender == contractOwner, "You are not the owner");

        _;

    }



    function ethUSD(uint256 _amount) public view returns (uint256) {

        (uint256 usd, uint8 dec) = getLatestPrice(dataFeedAddress);

        return (_amount * 10**dec) / usd;

    }



    function getLatestPrice(address assetAddress)

        internal

        view

        returns (uint256 price, uint8 dec)

    {

        AggregatorV3Interface priceFeed = AggregatorV3Interface(assetAddress);

        (, int256 answer, , , ) = priceFeed.latestRoundData();

        dec = priceFeed.decimals();

        return (uint256(answer), dec);

    }



    function setURI(uint256 poolID, string memory newuri) public onlyOwner {

        uris[poolID] = newuri;

    }



    function uri(uint256 poolID) public view override returns (string memory) {

        return uris[poolID];

    }



    function mint(uint256 id, uint256 amount) public payable {

        uint256 index = id - 1;

        // checks index if it exists

        require(nodeInfo[index].exists, "Token does not exist");

        // keeps total supply no more than max supply

        require(

            nodeInfo[index].totalSupply + amount <= nodeInfo[index].maxSupply,

            "Total Supply out of bounds"

        );

        // requires msg.sender to send enough funds

        uint256 price = ethUSD(nodeInfo[index].price);

        require(msg.value == price * amount, "Incorrect value");

        uint256 pricePerAsset = msg.value / amount;

        if (

            pricePerAsset >= ((100000000 + slippage) * price) / 100000000 ||

            pricePerAsset <= ((100000000 - slippage) * price) / 100000000

        ) {

            revert(string(abi.encodePacked("Slippage Error")));

        }

        // inherited function

        _mint(msg.sender, id, amount, "");

        // adds the minted amount to total supply

        nodeInfo[index].totalSupply += amount;

    }



    function viewTotalSupply(uint256 id) public view returns (uint256) {

        uint256 index = id - 1;

        require(nodeInfo[index].exists, "This token does not exist");

        return nodeInfo[index].totalSupply;

    }



    function viewMaxSupply(uint256 id) public view returns (uint256) {

        uint256 index = id - 1;

        // checks if index if it exists

        require(nodeInfo[index].exists, "Token does not exist");

        // returns

        return nodeInfo[index].maxSupply;

    }



    function withdraw() public onlyOwner {

        // Verifies amount of ETH in contract address

        require(address(this).balance > 0, "Cannot withdraw a balance of 0");

        // calls the withdraw method

        payable(contractOwner).transfer(address(this).balance);

    }

}