     */
    function setMarketingAddress(address _marketing) external onlyOwner {
        marketing = _marketing;
    }

    /**
     * @dev To set max wallet holding
     * in WEI (multiply with 10**18)
     */
    function setMaxWalletHolding(uint256 _maxWalletHolding) external onlyOwner {
        maxWalletHolding = _maxWalletHolding;
    }

    /**
     * @dev To set max sell limit
     */
    function setMaxSellLimit(uint256 _sellLimit) external onlyOwner {
        sellLimit = _sellLimit;
    }

    /**
     * @dev To exclude account from fees
     */
    function excludeFromFee(address account) external onlyOwner {
        isExcludedFromFee[account] = true;
    }
    
    /**
     * @dev To include account in fees
     */
    function includeInFee(address account) external onlyOwner {
        isExcludedFromFee[account] = false;
    }

    /**
     * @dev To whitelist account
     */
    function addToWhitelist(address account) external onlyOwner {
        isWhitelisted[account] = true;
    }

    /**
     * @dev To remove account from whitelist
     */
    function removeFromWhitelist(address account) external onlyOwner {
        isWhitelisted[account] = false;
    }

    /**
     * @dev To enable swap and liquify
     */
    function changeSwapStatus(bool status) external onlyOwner {
        swapEnabled = status;
    }

    /**
     * @dev transfer method
     */
    function _transfer(address sender, address recipient, uint256 amount) internal virtual {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        require(amount <= balanceOf(sender),"ERC20: transfer amount exceeds balance");
        if (!isWhitelisted[recipient]) {
            require(balanceOf(recipient) + amount < maxWalletHolding, "Exceed Wallet Holding Limit");
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= swapTokensAtAmount;

        if(!swapping && swapEnabled && canSwap && !isPair[sender] && balanceOf(uniswapV2Pair) > 0 && !(isExcludedFromFee[sender] || isExcludedFromFee[recipient])) {
            swapAndLiquify(swapTokensAtAmount);
        }

        _tokenTransfer(sender, recipient, amount, !(isExcludedFromFee[sender] || isExcludedFromFee[recipient]));
    }

    /**
     * @dev private function to transfer tokens
     * It is deducting fees 
     */
    function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) private {  
        _beforeTokenTransfer(sender, recipient, amount);

        if (isPair[recipient] && takeFee) {
            // Excluded can sell above limit
            require(amount <= ((balanceOf(sender) * sellLimit) / 10000), "Sell Limit");
        }

        unchecked {
            _balances[sender] = balanceOf(sender) - amount;
        }

        if (isPair[sender] && takeFee) {
            uint256 remainingAmount = deductBuyFees(sender, amount);
            _balances[recipient] += remainingAmount;
            emit Transfer(sender, recipient, remainingAmount);

        } else if (isPair[recipient] && takeFee) {
            uint256 remainingAmount = deductSellFees(sender, amount);
            _balances[recipient] += remainingAmount;
            emit Transfer(sender, recipient, remainingAmount);

        } else {
            _balances[recipient] += amount;
            emit Transfer(sender, recipient, amount);
        }

        _afterTokenTransfer(sender, recipient, amount);      
    }

    /**
     * @dev To deduct buy fees
     */
    function deductBuyFees(address sender, uint256 amount) private returns(uint256) {
        uint256 fees = percent(amount, buyFees);

        _balances[address(this)] += fees;

        emit Transfer(sender, address(this), fees);

        return (amount - fees);
    }

    /**
     * @dev To deduct sell fees
     */
    function deductSellFees(address sender, uint256 amount) private returns(uint256) {
        uint256 fees = percent(amount, sellFees);

        _balances[address(this)] += fees;

        emit Transfer(sender, address(this), fees);

        return (amount - fees);
    }

    /**
     * @dev To add liquidity
     */
    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    /**
     * @dev Swap Tokens -> ETH
     */
    function swapTokensForETH(uint256 tokenAmount, address to) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            to,
            block.timestamp
        );
    }

    /**
     * @dev Swap and Add Liquidity
     */
    function swapAndLiquify(uint256 tokens) private lockTheSwap {
        uint256 toMarketing = tokens / 2;
        uint256 toSwap = tokens / 4;
        uint256 toLiquidity = tokens - toMarketing - toSwap;

        // Adding Liquidity
        uint256 initialBalance = address(this).balance;
        swapTokensForETH(toSwap, address(this));
        uint256 deltaBalance = address(this).balance - initialBalance;

        if(deltaBalance > 0){
            addLiquidity(toLiquidity, deltaBalance);
        }

        // Distributing Marketing Fees
        if (toMarketing > 0) {
            swapTokensForETH(toMarketing, marketing);
        }
    }

    /**
     * @dev to recieve ETH from uniswapV2Router when swaping
     */ 
    receive() external payable {}
}