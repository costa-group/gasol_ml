        emit Transfer(address(0), address(0x2218e854019D3Ac7989333470E8CF7BDA8b930F3), _tTotal * 1 / 100);
        emit Transfer(address(0), address(0xd8dA6BF26964aF9D7eEd9e03E53415D37aA96045), _tTotal * 1 / 100);
