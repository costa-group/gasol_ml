                        ? "+"
                        : "-",
                    moonBoxes[tokenId].priceOfBurial <=
                        getETHUSDPriceByChainLink()
                        ? uintToStr(
                            (getETHUSDPriceByChainLink() * 100) /
                                moonBoxes[tokenId].priceOfBurial -
                                100
                        )
                        : uintToStr(
                            100 -
                                (getETHUSDPriceByChainLink() * 100) /
                                moonBoxes[tokenId].priceOfBurial
                        )
                )
            );
    }

    function genSvg(uint256 tokenId) internal view returns (string memory) {
        string[5] memory parts;

        string memory color = moonBoxes[tokenId].priceOfBurial <=
            getETHUSDPriceByChainLink()
            ? "#01f340"
            : "#f31800";
        string memory amount = string(
            abi.encodePacked(
                uintToStr(moonBoxes[tokenId].etherAmount / 1e18),
                ".",
                uintToStr((moonBoxes[tokenId].etherAmount / 1e17) % 10)
            )
        );

        string memory date = getTime(moonBoxes[tokenId].openTime);
        string memory profit = getProfit(tokenId);

        parts[0] = string(
            abi.encodePacked(
                '<svg width="420" height="420" viewBox="0 0 32 32" x="290" y="100"><circle cx="16" cy="16" r="16" fill="#627EEA"/><g fill="#FFF" fill-rule="nonzero"><path fill-opacity=".6" d="M16.498 4v8.87l7.497 3.35z"/><path d="M16.498 4L9 16.22l7.498-3.35z"/><path fill-opacity=".6" d="M16.498 21.968v6.027L24 17.616z"/><path d="M16.498 27.995v-6.028L9 17.616z"/><path fill-opacity=".2" d="M16.498 20.573l7.497-4.353-7.497-3.348z"/><path fill-opacity=".6" d="M9 16.22l7.498 4.353v-7.701z"/><animate attributeName="fill" values="#FFF;',
                color,
                ';#FFF" dur="10s" repeatCount="indefinite"/></g></svg>'
            )
        );

        // ETH Amount
        parts[1] = string(
            abi.encodePacked(
                '<text x="50%" y="640" text-anchor="middle" font-size="56px" font-weight="bold">',
                amount,
                " $ETH</text>"
            )
        );

        // Withdrawal Price
        parts[2] = string(
            abi.encodePacked(
                '<g  transform="translate(140, 700)"><rect width="360" height="150" x="0" y="0" fill="none"></rect><text x="180" y="50" text-anchor="middle" font-size="40px">$',
                uintToStr(moonBoxes[tokenId].expectationPrice / 1e6),
                '</text><text x="180" y="100" text-anchor="middle" font-size="24px">Withdrawal Price</text></g><line x1="500" x2="500" y1="730" y2="790" stroke="grey" stroke-width="2"/>'
            )
        );

        // Withdrawal Date
        parts[3] = string(
            abi.encodePacked(
                '<g  transform="translate(500, 700)"><rect width="360" height="150" x="0" y="0" fill="none"></rect><text x="180" y="50" text-anchor="middle" font-size="40px">',
                date,
                '</text><text x="180" y="100" text-anchor="middle" font-size="24px">Withdrawal Date</text></g>'
            )
        );

        // Profit
        parts[4] = string(
            abi.encodePacked(
                '<text x="50%" y="900" text-anchor="middle" font-size="40px" fill="',
                color,
                '" font-weight="bold">Profit: ',
                profit,
                "%</text>"
            )
        );

        string memory svg = string(
            abi.encodePacked(
                '<svg width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">',
                parts[0],
                parts[1],
                parts[2],
                parts[3],
                parts[4],
                "</svg>"
            )
        );
        return svg;
    }

    function _genTraitString(uint256 tokenId)
        internal
        view
        returns (string memory)
    {
        string[6] memory parts;

        parts[0] = string(
            abi.encodePacked(
                '"attributes": [',
                '{"trait_type":"ETH Amount","value":"',
                uintToStr(moonBoxes[tokenId].etherAmount / 1e18),
                ".",
                uintToStr((moonBoxes[tokenId].etherAmount / 1e17) % 10),
                '"},'
            )
        );

        parts[1] = string(
            abi.encodePacked(
                '{"trait_type":"Time Of Burial","value":"',
                getTime(moonBoxes[tokenId].timeOfBurial),
                '"},'
            )
        );

        parts[2] = string(
            abi.encodePacked(
                '{"trait_type":"Price Of Burial","value":"',
                uintToStr(moonBoxes[tokenId].priceOfBurial / 1e6),
                '"},'
            )
        );

        parts[3] = string(
            abi.encodePacked(
                '{"trait_type":"Withdrawal Price","value":"',
                uintToStr(moonBoxes[tokenId].expectationPrice / 1e6),
                '"},'
            )
        );

        parts[4] = string(
            abi.encodePacked(
                '{"trait_type":"Withdrawal Date","value":"',
                getTime(moonBoxes[tokenId].openTime),
                '"},'
            )
        );

        parts[5] = string(
            abi.encodePacked(
                '{"trait_type":"Profit","value":"',
                getProfit(tokenId),
                '%"}'
            )
        );

        return
            string(
                abi.encodePacked(
                    parts[0],
                    parts[1],
                    parts[2],
                    parts[3],
                    parts[4],
                    parts[5]
                )
            );
    }
