//pragma solidity ^0.8.19;


contract TimeCapsule  {
  using SafeMath for uint;   

    MintBadge mintbadge_ = MintBadge(0x73b8CeB6D96202c37D2d168931D3e4D4F33e8c7D);
    
  
    // Start Here -- ↓↓↓↓↓↓ ---
   
    uint256 private value; 
   

    constructor(){       
        value = block.number.mul(145e4);       
    }


    function Crack(bytes32 _key) external  {
        uint Secret = value.div(block.number);
        bytes32 key = keccak256(abi.encodePacked(Secret));
        require(_key == key);
        mintbadge_.Mint(msg.sender,3,1);
    }


    // -----End---- ↑↑↑↑↑↑ ---




    // -- Just tool -- 

    function HASH(uint256 number)external pure returns(bytes32){
        bytes32 ciphertext = keccak256(abi.encodePacked(number));
        return ciphertext;
    }
   
  

}